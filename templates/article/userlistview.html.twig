{% for article in articles %}
                    <div class="col mb-5">
                        <div class="card h-100">
                            
                            
                            <!-- Product image failed-->
                            <img class="card-img-top" src="https://picsum.photos/450/300" alt="..." />
                            <!-- Product details-->
                            <p>
                                {{ article.getTeaser() }}
                            </p>
                            <div class="card-body p-4">
                                <div class="text-center">
                                    <!-- Product name-->
                                    <h5 class="fw-bolder">{{ article.getName() | upper  }}</h5>
                                    
                                    
                                    
                                    
                                    <div class="d-flex justify-content-center small text-warning mb-2">
                                        

                                    </div>
                                    

                                    <!-- Product price-->
                                    <span class="text-muted text-decoration-line-through"></span>
                                    {{ article.getPrice() * 0.012 }} â‚¬
                                </div>
                            </div>
                            <!-- Product actions-->
                            <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                                <div class="text-center"><a class="btn btn-outline-dark mt-auto" href="">Ajouter au panier</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    {% endfor %}
